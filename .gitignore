function ShowCreditsNum():void
{
	var num:int;
	num = credits;
	if(num > 999999)
	{
		Object(root).screen_game.mc_game_M.txt_credits_num.text = String(int(num/1000000)+","+int((num%1000000)/100000)+int((num%100000)/10000)+int((num%10000)/1000)+","+int((num%1000)/100)+int((num%100)/10)+int(num%10));
	}else if(num > 999)
	{
		Object(root).screen_game.mc_game_M.txt_credits_num.text = String(int(num/1000)+","+int((num%1000)/100)+int((num%100)/10)+int(num%10));
	}else
	{
		Object(root).screen_game.mc_game_M.txt_credits_num.text = String(num);
	}
}

function ShowBetNum():void
{
	switch(play_gold_symbols){
		case 1:
