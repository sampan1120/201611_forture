function playBGM(sound:Sound):void
{
	BGMChannel.stop();
	BGMChannel = sound.play(0,int.MAX_VALUE);
}

function playItemSound(sound:Sound,playTimes:uint):void
{
	itemChannel.stop();
	itemChannel = sound.play(0,playTimes);
}

function playLuoSound(sound:Sound,playTimes:uint):void
{
	//luoChannel.stop();
	luoChannel = sound.play(0,playTimes);
}

function playEffectSound(sound:Sound,playTimes:uint):void
{
	effectChannel.stop();
	effectChannel = sound.play(0,playTimes);
}


