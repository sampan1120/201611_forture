//开始游戏
function StartGameFunction(e:MouseEvent):void
{
	if((credits > bet && !game_start)|| (credits == bet && !game_start))
	{
		game_start = true;
	}
}
//自动开始游戏
function AutoGameFunction(e:MouseEvent):void
{
	if((credits > bet && !auto_spin)|| (credits == bet && !auto_spin))
	{
		auto_spin = true;
		Object(root).screen_game.mc_game_M.btn_stop.visible = true;
		Object(root).screen_game.mc_game_M.btn_play.visible = false;
		Object(root).screen_game.mc_game_M.mc_auto_play_dis.visible = true;
	}
}
//结束自动游戏
function StopAutoGameFunction(e:MouseEvent):void
{
	auto_spin = false;
	Object(root).screen_game.mc_game_M.btn_stop.visible = false;
	Object(root).screen_game.mc_game_M.btn_play.visible = true;
	Object(root).screen_game.mc_game_M.mc_auto_play_dis.visible = false;
}
