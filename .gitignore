//监听舞台更新
addEventListener(Event.ENTER_FRAME, UpdateWorld);

function UpdateWorld(e:Event):void
{
	//如果游戏没有运行，开始游戏。并开始计时，准备下一轮游戏
	//开始按钮触发游戏
	if (game_start && !reel_spining)
	{
		//更新资产
		credits -= bet;
		ShowCreditsNum();
		//重置赢的钱数
		the_num_of_win = 0;
		ShowWinNum();
		//停止所有声音
		itemChannel.stop();
		effectChannel.stop();
		luoChannel.stop();
		
		//生成新的item数组
		NewItemNum();
		//reel开始转动
		ReelBeginSpin();
		//reel为转动状态
		reel_spining = true;
		
		//item正常停止时的声音
		play_reel_stop_sound_timer.start();
		//播放一轮游戏结束时，计算结果
		reel_stop_timer.start();
		
		//初始化游戏
		reset_timer.start();
	}
	//自动游戏
	if (auto_spin && !reel_spining)
	{
		if(bet > credits)
		{
			auto_spin = false;
			Object(root).screen_game.mc_game_M.btn_stop.visible = false;
			Object(root).screen_game.mc_game_M.btn_play.visible = true;
			Object(root).screen_game.mc_game_M.mc_auto_play_dis.visible = false;
		}else
		{
			//更新资产
			credits -= bet;
			ShowCreditsNum();
			//重置赢的钱数
			the_num_of_win = 0;
			ShowWinNum();
			//停止所有声音
			itemChannel.stop();
			effectChannel.stop();
			luoChannel.stop();
			
			//生成新的item数组
			NewItemNum();
			//reel开始转动
			ReelBeginSpin();
			//reel为转动状态
			reel_spining = true;
			
			//item正常停止时的声音
			play_reel_stop_sound_timer.start();
			//播放一轮游戏结束时，计算结果
			reel_stop_timer.start();
			
			//如果赢了，播放完音乐之后再进行，否则直接继续下一局
			if(the_num_of_win > 0)
			{
				auto_win_reset_timer.start();
			}
			else
			{
				auto_reset_timer.start();
			}
		}
	}
}

