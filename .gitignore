			}
			if(the_kind_num_of_item_win_temp == 2)
			{
				Play2ItemAniFunction(item_num,4);
			}
			if(the_kind_num_of_item_win_temp == 3)
			{
				Play3ItemAniFunction(item_num,4);
			}
		}
		
	}
	else if (the_same_num_of_item_in_1_vertical > 0 && the_same_num_of_item_in_2_vertical > 0 && the_same_num_of_item_in_3_vertical > 0)
	{
		//item_same_num = the_same_num_of_item_in_1_vertical + the_same_num_of_item_in_2_vertical + the_same_num_of_item_in_3_vertical;
		item_line_num = the_same_num_of_item_in_1_vertical * the_same_num_of_item_in_2_vertical * the_same_num_of_item_in_3_vertical;
		
		the_num_of_win += pay_fortune_3 * item_line_num;
		
		the_kind_num_of_item_win_temp ++	;
		
		if(item_16_col_of_win > 0)
		{
			//Play1ItemAniFunction(item_num,5);
		}else{
			if(the_kind_num_of_item_win_temp == 1)
