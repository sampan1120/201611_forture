}

//play the reel stop sound function包括赢和没赢的声音
var play_reel_stop_sound_timer:Timer = new Timer(1200, 1);
play_reel_stop_sound_timer.addEventListener(TimerEvent.TIMER_COMPLETE, PlayReelStopSoundTimerFunction);

function PlayReelStopSoundTimerFunction(event:TimerEvent):void
{
	
	//检查是否有锣,有多少，响几次
	var the_1_col_luo_item:int;
	var the_2_col_luo_item:int;
	var the_3_col_luo_item:int;
	var the_4_col_luo_item:int;
	var the_5_col_luo_item:int;
	the_1_col_luo_item = 0;
	the_2_col_luo_item = 0;
	the_3_col_luo_item = 0;
	the_4_col_luo_item = 0;
	the_5_col_luo_item = 0;
	//检查2、3、4列是否有福
	var the_2_col_fu_item:Boolean;
	var the_3_col_fu_item:Boolean;
	var the_4_col_fu_item:Boolean;
	the_2_col_fu_item = false;
	the_3_col_fu_item = false;
	the_4_col_fu_item = false;

	for(var i:int = 1; i < 4; i++)
	{
		//luo item state
		if (new_array_reel_belt_01[i] == 16)
		{
			the_1_col_luo_item = 1;
		}

		if (new_array_reel_belt_02[i] == 16)
		{
			the_2_col_luo_item = 1;
		}

		if (new_array_reel_belt_03[i] == 16)
		{
			the_3_col_luo_item = 1;
		}

		if (new_array_reel_belt_04[i] == 16)
		{
			the_4_col_luo_item = 1;
		}

		if (new_array_reel_belt_05[i] == 16)
		{
			the_5_col_luo_item = 1;
		}

		//fu item state
		if (new_array_reel_belt_02[i] == 17)
		{
			the_2_col_fu_item = true;
		}

		if (new_array_reel_belt_03[i] == 17)
		{
			the_3_col_fu_item = true;
		}

		if (new_array_reel_belt_04[i] == 17)
		{
			the_4_col_fu_item = true;
		}
	}
	
	//5个锣,不播放item停止的声音。后续有free game
	if(the_1_col_luo_item == 1 && (the_2_col_luo_item == 1|| the_2_col_fu_item) && (the_3_col_luo_item == 1|| the_3_col_fu_item) && (the_4_col_luo_item == 1|| the_4_col_fu_item) && the_5_col_luo_item == 1)
	{
		play_1_luo_sound_timer.start();
		if(the_2_col_luo_item == 1)
		{
			play_2_luo_sound_timer.start();
		}
		
		if(the_3_col_luo_item == 1)
		{
			play_3_luo_sound_timer.start();
		}
		
		if(the_4_col_luo_item == 1)
		{
			play_4_luo_sound_timer.start();
		}
		play_5_luo_sound_timer.start();

		PlayFreeGameEffectSound(5);
		//playLuoSound(luo_01_sound,1);
		//playLuoSound(luo_02_sound,the_1_col_luo_item + the_2_col_luo_item + the_3_col_luo_item + the_4_col_luo_item + the_5_col_luo_item);

	}
	//4个锣,不播放item停止的声音。后续有free game
	else if(the_1_col_luo_item == 1 && (the_2_col_luo_item == 1|| the_2_col_fu_item) && (the_3_col_luo_item == 1|| the_3_col_fu_item) && (the_4_col_luo_item == 1|| the_4_col_fu_item))
	{
		play_1_luo_sound_timer.start();
		
		if(the_2_col_luo_item == 1)
		{
			play_2_luo_sound_timer.start();
		}
		
		if(the_3_col_luo_item == 1)
		{
			play_3_luo_sound_timer.start();
		}
		
		if(the_4_col_luo_item == 1)
		{
			play_4_luo_sound_timer.start();
		}

		PlayFreeGameEffectSound(4);
	}
	//3个锣,不播放item停止的声音。后续有free game
	else if(the_1_col_luo_item == 1 && (the_2_col_luo_item == 1|| the_2_col_fu_item) && (the_3_col_luo_item == 1|| the_3_col_fu_item))
	{
		play_1_luo_sound_timer.start();
		
		if(the_2_col_luo_item == 1)
		{
			play_2_luo_sound_timer.start();
		}
		
		if(the_3_col_luo_item == 1)
		{
			play_3_luo_sound_timer.start();
		}
		
		PlayFreeGameEffectSound(3);
	}
	//2个锣
	else if(the_1_col_luo_item == 1 && (the_2_col_luo_item == 1|| the_2_col_fu_item))
	{
		play_1_luo_sound_timer.start();
		
		if(the_2_col_luo_item == 1)
		{
			play_2_luo_sound_timer.start();
		}

		playItemSound(reel_stop_nor_sound, 1);
	}
	//1个锣
	else if(the_1_col_luo_item == 1)
	{
		play_1_luo_sound_timer.start();

		playItemSound(reel_stop_nor_sound, 1);
	}else
	{
		playItemSound(reel_stop_nor_sound, 1);
	}
	

}

//每一个col停止间隔时间
var luo_sound_time:int = 310;

