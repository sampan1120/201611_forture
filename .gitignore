function SetCreditPerPlayUpFunction(e:MouseEvent):void
{
	
	if(credits_per_play == 1)
	{
		credits_per_play = 2;
		Object(root).screen_game.mc_game_M.mc_credits_status.gotoAndStop(2);
		ShowBetNum();
	}else if(credits_per_play == 2)
	{
		credits_per_play = 3;
		Object(root).screen_game.mc_game_M.mc_credits_status.gotoAndStop(3);
